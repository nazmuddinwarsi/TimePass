@* @model RohatasTransportCore.Models.test *@
@model List<TimePass.Models.RegistrationList>
@{
	@foreach (var RType in Model)
	{
		ViewBag.Title =  " Registration List";
		 
	}

	 
}
<section class="body">

	<!-- start: header -->
	<header class="header">
		@* <uc1:AdminHeader ID="AdminHeader1" runat="server" /> *@
		@Html.Partial("UserHeader")
		<!-- start: search & user box -->
		<!-- end: search & user box -->
	</header>
	<!-- end: header -->

	<div class="inner-wrapper">
		<!-- start: sidebar -->
		@* @{
		Html.RenderPartial("_AdminMenu");
		} *@
		@Html.Partial("AdminMenu")
		<!-- end: sidebar -->
		<div>

			<!-- Modal Basic -->
			<!-- Modal Bootstrap -->

		</div>

		<section role="main" class="content-body card-margin">


			<!-- start: page -->


			<div class="row">

				<div class="col-lg-12">
					<section class="card form-wizard" id="w1">
						<header class="card-header">
							<div class="card-actions">
								<a href="#" class="card-action card-action-toggle" data-card-toggle></a>
								<a href="#" class="card-action card-action-dismiss" data-card-dismiss></a>
							</div>
							@* @foreach (var RType in Model) *@

							<h2 class="card-title">  Registration List  </h2>


						</header>
						<div class="card-body card-body-nopadding">
							<ul class="nav nav-pills nav-pills-primary">
								&nbsp;
								<li class="nav-item">

									@* <a class="nav-link active" href="Registation?tTpye"=@RType.TType">Add New</a> *@
									<a asp-controller="Admin" asp-action="Registation" class="nav-link active"  >Add New</a>

								</li>



							</ul>
							<div id="w1-account" class="tab-pane p-3 ">
								<div>
									<table cellspacing="0" rules="all" class="table table-bordered table-striped mb-0" border="1" id="grd" style="border-collapse:collapse;">
										<thead>
											<tr class="headerStyle">
												<th scope="col">Sr.</th>
												<th scope="col">Name</th>
												<th scope="col">Mobile</th>
												<th scope="col">Address</th>
												<th scope="col">UserName</th>
												<th scope="col">Password</th>
												<th scope="col">Reg Date</th>
												<th scope="col">Coins</th>
												<th scope="col">Trans</th>
												 
												<th align="center" scope="col" style="width:50px;">Edit</th>
												<th align="center" scope="col" style="width:50px;">Delete</th>
											</tr>
										</thead>
										<tbody>
											@if (Model != null && Model.Any())
											{
												@* @Model.stateLists = new  *@
												var i = 0;
												@foreach (var Reg in Model)
												{
													var moid = "modal" + Reg.UserId;
													i = i + 1;
													var ActiveColor = "";
												 
													<tr class="rowStyle" style="background-color:@ActiveColor">
														<td style="white-space:nowrap;">@i</td>
														<td style="white-space:nowrap;">@Reg.Name</td>
														<td style="white-space:nowrap;">@Reg.Mobile</td>
														<td style="white-space:nowrap;">@Reg.Address</td>
														<td style="white-space: nowrap">@Reg.Username</td>
														<td style="white-space:nowrap;">@Reg.Password</td>
														<td style="white-space:nowrap;">@Reg.CreatedDate.Value.ToShortDateString()</td>
														<td style="white-space:nowrap;">@Reg.Coins</td>

														<td style="white-space:nowrap;">

															<center><a asp-controller="Admin" asp-action="Transaction" asp-route-id="@Reg.UserId"><i class="fas fa-exchange-alt" style="color:black" aria-hidden="true"></i></a></center>
															 
														</td>
														 

														<td align="center">
															<a asp-controller="Admin" asp-action="Registation" asp-route-id="@Reg.UserId"><img src="~/../images/img_edit.png" /></a>



														</td>
														<td align="center">

															<a class="mb-1 mt-1 mr-1 modal-basic" href="#@moid"><img src="~/../images/img_delete.png" /></a>


														</td>
													</tr>
													<div id="@moid" class="modal-block mfp-hide">
														<section class="card">
															<header class="card-header">
																<h2 class="card-title">Are you sure?</h2>
															</header>
															<div class="card-body">
																<div class="modal-wrapper">
																	<div class="modal-text">
																		<p class="mb-0">Are you sure that you want to delete this data?</p>
																	</div>
																</div>
															</div>
															<footer class="card-footer">
																<div class="row">
																	<div class="col-md-12 text-right">
																		<a asp-controller="Admin" asp-action="RegistrationList" class="btn btn-primary" asp-route-id="@Reg.UserId">Confirm</a>


																		<button class="btn btn-default modal-dismiss">Cancel</button>
																	</div>
																</div>
															</footer>
														</section>
													</div>
												}

											}
										</tbody>
										<tfoot>
										</tfoot>
									</table>
								</div>
							</div>
						</div>

					</section>
				</div>
			</div>
			<!-- end: page -->
		</section>
	</div>


</section>